; port functions placed in ROM


(define newline
  (lambda args
    (if (null? args)
	(write-char #\newline)
	(write-char #\newline (car args))
	)
    )
  )

(newline)
(newline (open-output-file "/dev/null"))

(define (load name)
  (let ((p (open-input-file name))
	(e))
    (while (not (eof-object? (set! e (read p))))
	   (write (eval e)) (newline)
	   )
    (close-port p)
    )
  )

(load "/dev/null")
